{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Warning",
      "DurableMediator": "Information",
      "WorkflowFunctionApp": "Information"
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "DurableWorkflowHub",
      "useGracefulShutdown": true
    },
    "http": {
      "routePrefix": ""
    }
  }
}